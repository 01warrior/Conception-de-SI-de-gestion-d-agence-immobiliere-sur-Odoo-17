<odoo>
    <data noupdate="1">
        <record id="seq_property_property" model="ir.sequence">
            <field name="name">Séquence Propriété Immobilière</field>
            <field name="code">property.property.sequence</field> 
            <field name="prefix">PROP/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- SÉQUENCE AUTO POUR LES MANDATS -->
        <record id="seq_property_mandate" model="ir.sequence">
            <field name="name">Séquence Mandat Propriété</field>
            <field name="code">property.mandate.sequence</field>
            <field name="prefix">MAN/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

         <!--  SÉQUENCE POUR LES VISITES -->
        <record id="seq_property_visit" model="ir.sequence">
            <field name="name">Séquence Visite Propriété</field>
            <field name="code">property.visit.sequence</field> <!-- Doit correspondre au code dans le Python -->
            <field name="prefix">VISIT/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>


        <!-- SÉQUENCE POUR LES OFFRES -->
        <record id="seq_property_offer" model="ir.sequence">
        <field name="name">Séquence Offre Propriété</field>
        <field name="code">property.offer.sequence</field>
        <field name="prefix">OFFER/</field>
        <field name="padding">6</field>
        <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>