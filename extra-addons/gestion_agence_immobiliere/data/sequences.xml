<odoo>
    <data noupdate="1">
        <record id="seq_property_property" model="ir.sequence">
            <field name="name">Séquence Propriété Immobilière</field>
            <field name="code">property.property.sequence</field> 
            <field name="prefix">PROP/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- SÉQUENCE AUTO POUR LES MANDATS -->
        <record id="seq_property_mandate" model="ir.sequence">
            <field name="name">Séquence Mandat Propriété</field>
            <field name="code">property.mandate.sequence</field>
            <field name="prefix">MAN/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>